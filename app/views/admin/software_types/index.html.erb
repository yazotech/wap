<h1>Listing software_types</h1>

<table>
  <tr>
    <th>ID</th>
    <th>分类</th>
    <th>标题</th>
    <th>状态</th>
    <th>操作</th>
  </tr>

<% @software_types.each do |software_type| %>
  <tr>
    <td><%= software_type.id %></td>
   <td><%= software_type.parent.title if software_type.parent %></td>
    <td><%= software_type.title %></td>
    <td><%= software_type.status %></td>
    <td>
      <%= link_to '显示', admin_software_type_path(software_type) %> |
      <%= link_to '修改', edit_admin_software_type_path(software_type) %> |
      <%= link_to '删除', admin_software_type_path(software_type), :confirm => '确认删除吗？', :method => :delete %>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to '添加新分类', new_admin_software_type_path %>
