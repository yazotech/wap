<h1>Listing photos</h1>

<table>
  <tr>
    <th>Software</th>
    <th>Photo name</th>
    <th>Photo type</th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.software_id %></td>
    <td><%= photo.photo_name %></td>
    <td><%= photo.photo_type %></td>
    <td><%= link_to 'Show', admin_photo_path(photo) %></td>
    <td><%= link_to 'Edit', edit_admin_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', admin_photo_path(photo), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_admin_photo_path %>
